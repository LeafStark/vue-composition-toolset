{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["dom", "ESNext"],
    "paths": {
      "vue-composition-toolset": ["./src/index.ts"]
    },
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "declaration": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "noUnusedLocals": true
  }
}
